{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:63767",
      "sslPort": 44389 //sslPort свойство, которое указывает, что наше приложение при работе в IISExpress также будет настроено для HTTPS (порт 44370)
    }
  },
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false, //запуск браузера при старте. Запустится "launchUrl": "weatherforecast"
      "launchUrl": "weatherforecast", //Это свойство определяет, к какому URL-адресу приложение будет переходить изначально
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "CompanyEmployees": {
      "commandName": "Project",
      "launchBrowser": false, //запуск браузера.Запустится "launchUrl": "weatherforecast"
      "launchUrl": "weatherforecast", //Это свойство определяет, к какому URL-адресу приложение будет переходить изначально
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
